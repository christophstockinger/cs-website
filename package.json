{
  "name": "cs-website",
  "version": "7.0.0",
  "description": "Personal website of Christoph Stockinger",
  "author": "Christoph Stockinger <cs@christophstockinger.de>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophstockinger/cs-website.git"
  },
  "bugs": {
    "url": "https://github.com/christophstockinger/cs-website/issues"
  },
  "homepage": "https://www.christophstockinger.de",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rimraf .nuxt",
    "stylelint": "stylelint **/*.{css,scss}",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . && npm run stylelint",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint:commit": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/svg-sprite": "^0.4.9",
    "nuxt": "^2.0.0",
    "nuxt-babel": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "git-branch-is": "^3.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.14.2",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is -r \"^((?!master).)*$\" && lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint",
    "src/**/*.{css,scss}": "stylelint"
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "IE 11"
  ]
}
