  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint",
    "src/**/*.{css,scss}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  }
}
