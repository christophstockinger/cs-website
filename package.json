{
  "name": "cs-website",
  "version": "7.1.2",
  "description": "My personal website <http://www.christophstockinger.de>",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm run clean && webpack-dev-server --env.development",
    "build": "npm run clean && npm run production",
    "dev": "webpack --env.development",
    "production": "webpack --env.production",
    "deploy": "npm run build && firebase deploy",
    "clean": "rimraf ./dist",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "lint": "eslint './src/**/*.js' --config .eslintrc.json",
    "lint:fix": "prettier-eslint './src/**/*.js' --write",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "lint:commit": "commitlint -e $GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophstockinger/cs-website.git"
  },
  "author": "Christoph Stockinger <cstockinger@coding77.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christophstockinger/cs-website/issues"
  },
  "homepage": "https://github.com/christophstockinger/cs-website#readme",
  "dependencies": {
    "body-scroll-lock": "^3.0.3",
    "universal-cookie": "^4.0.3",
    "vue": "^3.0.0-beta.18",
    "vue-router": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@vue/compiler-sfc": "^3.0.0-beta.18",
    "autoprefixer": "^9.8.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-prettier-vue": "^2.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.0-alpha.9",
    "file-loader": "^6.0.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-config-standard": "^1.0.1",
    "prettier-standard": "^16.3.0",
    "pretty": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.1",
    "sass-resources-loader": "^2.0.3",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recess-order": "^2.0.4",
