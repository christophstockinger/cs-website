{% use './../config.html.twig' %}

<!DOCTYPE html>
<html prefix="og: http://ogp.me/ns#"  lang="en">
<head>
  <meta charset="UTF-8">
  <title>{{ pageTitle }}</title>
  <meta name="description" content="" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta property="og:description" content="" />
  <meta property="og:image" content="/assets/images/christoph-stockinger.jpg" />
  <meta property="og:locale" content="de_DE" />
  <meta property="og:title" content="{{ pageTitle }}" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="{{pageUrl}}" />

  {% if (environment == 'production') %}
  <link rel="stylesheet" href="/assets/main.bundle.css" type="text/css">
  {% endif %}

</head>
<body>
  {% block body %}
  {% endblock body %}

  {% if (environment == 'production') %}
  <div class="uSprite" data-href="/assets/sprite.svg" data-revision="{{dataRevision}}"></div>
  {% endif %}

  <script src="/assets/main.bundle.js" type="text/javascript"></script>

  {% if (environment == 'development') %}
  <script src="/assets/dev.bundle.js" type="text/javascript"></script>
  {% endif %}
</body>
</html>
