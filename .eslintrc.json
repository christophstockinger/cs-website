{
  "extends": ["prettier-standard"],
  "plugins": ["babel", "prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}