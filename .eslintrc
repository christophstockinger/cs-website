{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": "error",
    "prettier/prettier": ["error"]
  },
  "plugins": ["babel", "prettier"],
  "extends": ["standard", "prettier-standard"]
}
